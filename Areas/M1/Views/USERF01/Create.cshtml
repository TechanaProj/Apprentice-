        function printData() {
            // Clone the preview container to create a copy without the buttons
            var clonedContainer = document.getElementById('previewContainer').cloneNode(true);

            // Create a new window or tab
            var printWindow = window.open('', '_blank');

            // Append the cloned container to the new window/tab
            printWindow.document.body.appendChild(clonedContainer);

            // Print the content
            printWindow.print();
